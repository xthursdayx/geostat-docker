[NGINX_LOG]
#Path for the log file (Nginx)
logpath = /nginx_logs/access.log

[GEOIP]
#Path for the GEOIP DB file
geoipdb = /app/GeoLite2-City.mmdb

[INFLUXDB]
# Database URL
host = ip_address
port = 8086

#Database name
database = db_name

# InfluxDB Auth
username = "username"
password = "password"

#  Measurement name
measurement = geodata
